{
  "expo": {
    "name": "Med-Assistant",
    "slug": "med-reminder",
    "version": "1.1.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0FDBD1"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/pulse.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.msyoka.medreminder"
    },
    "web": {
      "favicon": "./assets/Med.png"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "eas": {
        "projectId": "e4fac554-ada6-439e-b190-808c8d8ee2cb"
      }
    },
    "scheme": "medassisant",
    "plugins": [
      "expo-font"
    ]
  }
}
